  "description": "This project was bootstrapped with Fastify-CLI.",
  "type": "module",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "dotenvx run -- node --watch src/app.js",
    "prod": "dotenvx run -- pm2 start ecosystem.config.cjs",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "seed": "dotenvx run -- node src/scripts/init/index.js",
    "seed:reset": "dotenvx run -- node src/scripts/init/index.js --reset",
    "seed:list": "dotenvx run -- node src/scripts/init/index.js --list",
    "seed:clean": "dotenvx run -- node src/scripts/init/index.js --clean"
  },
  "keywords": [],
