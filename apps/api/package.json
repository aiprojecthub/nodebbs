    "@fastify/cors": "^11.2.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/multipart": "^9.4.0",
    "@fastify/oauth2": "^8.2.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/redis": "^7.2.0",
