    "fastify-plugin": "^5.0.0",
    "ioredis": "^5.8.2",
    "ipx": "^3.1.1",
    "jsonwebtoken": "^9.0.3",
    "ms": "^2.1.3",
    "nanoid": "^5.1.6",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.0",
    "pino": "^10.1.0",
